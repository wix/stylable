{
  "name": "@stylable/jest",
  "version": "1.0.3",
  "description": "Test your Stylable React components using Jest",
  "main": "dist/jest.js",
  "types": "dist/jest.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc",
    "lint": "tslint --project .",
    "test": "mocha -r @ts-tools/node/r \"test/**/*.spec.ts?(x)\" --watch-extensions ts,tsx"
  },
  "dependencies": {
    "@stylable/node": "^1.0.3",
    "@stylable/runtime": "^1.0.3"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/wix/stylable/tree/master/packages/jest",
  "author": "Wix.com",
  "license": "BSD-3-Clause"
}
