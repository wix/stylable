{
  "name": "stylable",
  "version": "5.4.9",
  "description": "CSS for Components",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "webpack-dev-server --hot --inline",
    "clean": "rimraf ./dist",
    "test": "yarn test:node",
