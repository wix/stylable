{
  "name": "@stylable/webpack-extensions",
  "version": "2.3.0",
  "description": "Experimental Stylable webpack plugins",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "build": "ts-build ./src --cjs --esm",
    "test": "mocha \"test/**/*.spec.ts?(x)\" --timeout 20000",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@stylable/core": "^2.3.0",
    "@stylable/node": "^2.3.0",
    "@stylable/webpack-plugin": "^2.3.0",
    "lodash.clonedeep": "^4.5.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "webpack": "^4.40.2",
    "webpack-sources": "^1.4.3"
  },
  "files": [
    "cjs",
    "esm",
    "src",
    "!src/tsconfig.json"
  ],
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/wix/stylable/tree/master/packages/webpack-extensions",
  "author": "Wix.com",
  "license": "BSD-3-Clause",
  "keywords": [
    "css",
    "stylable",
    "webpack",
    "webpack-plugin"
  ]
